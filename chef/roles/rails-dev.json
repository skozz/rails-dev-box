{
  "json_class": "Chef::Role",
  "name": "rails-dev",
  "description": "Install ruby on rails",
  "run_list": [
    "recipe[apt]",
    "recipe[build-essential]",
    "recipe[git]",
    "recipe[sqlite]",
    "recipe[mysql::client]",
    "recipe[mysql::ruby]",
    "recipe[mysql::server]",
    "recipe[memcached]",
    "recipe[redis]",
    "recipe[postgresql::server]",
    "recipe[phantomjs]",
    "recipe[ruby_build]",
    "recipe[rbenv::vagrant]",
    "recipe[rbenv::user]",
    "recipe[nodejs::install_from_binary]"
  ],
  "override_attributes": {
    "mysql": {
      "server_root_password": "",
      "server_repl_password": "",
      "server_debian_password": ""
    },
    "postgresql": {
      "password": {
        "postgres": ""
      }
    },
    "rbenv": {
      "user_installs": [
        {
          "user": "vagrant",
          "rubies": ["2.0.0-p247"],
          "global": "2.0.0-p247",
          "gems": {
            "2.0.0-p247": [
              { "name": "bundler" }
            ]
          }
        }
      ]
    }
  }
}
